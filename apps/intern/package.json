{
	"name": "@pensjonskalkulator-frontend-monorepo/intern",
	"version": "1.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite",
		"dev:q2": "concurrently \"vite --mode backend\" \"nodemon --watch server/ --exec 'pnpm build:server && NODE_ENV=development node --env-file=.env.development-q2 dist/server/server.js'\"",
		"build": "tsc && vite build",
		"build:server": "tsc -p tsconfig.server.json",
		"preview": "vite preview",
		"typecheck": "tsc --noEmit",
		"test": "echo 'No tests yet' && exit 0",
		"test:e2e": "echo 'No E2E tests yet' && exit 0",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix"
	},
	"dependencies": {
		"@navikt/ds-react": "8.4.0",
		"@navikt/ds-css": "8.4.1",
		"@navikt/aksel-icons": "^7.40.0",
		"@navikt/oasis": "^3.4.0",
		"@pensjonskalkulator-frontend-monorepo/api": "workspace:*",
		"@tanstack/react-query": "^5.90.20",
		"express": "^5.1.0",
		"express-rate-limit": "^8.2.1",
		"http-proxy-middleware": "^3.0.3",
		"react": "^19.2.4",
		"react-dom": "^19.2.4",
		"unleash-client": "^6.4.0",
		"winston": "^3.17.0"
	},
	"devDependencies": {
		"@pensjonskalkulator-frontend-monorepo/types": "workspace:*",
		"@vitejs/plugin-react": "^5.1.2",
		"concurrently": "^9.2.0",
		"msw": "^2.10.5",
		"nodemon": "^3.1.11",
		"vite": "^7.3.1"
	},
	"msw": {
		"workerDirectory": [
			"public"
		]
	}
}
